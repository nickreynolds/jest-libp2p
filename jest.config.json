{
    "moduleFileExtensions": ["ts", "tsx", "js", "jsx", "json", "jsonld", "cjs", "mjs", "node"],
    "collectCoverage": false,
    "collectCoverageFrom": [
      "packages/**/src/**/*.ts",
      "!**/examples/**",
      "!packages/cli/**",
      "!**/types/**",
      "!**/build/**",
      "!**/node_modules/**"
    ],
    "coverageReporters": ["text", "lcov", "json"],
    "coverageDirectory": "./coverage",
    "transform": {
      "\\.jsx?$": "babel-jest",
      "\\.tsx?$": "ts-jest"
    },
    "testMatch": ["**/tests/**/*.test.*"],
    "testEnvironment": "node",
    "automock": false
  }
  